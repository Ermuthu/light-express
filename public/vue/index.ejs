<script src="https://unpkg.com/vue/dist/vue.js"></script>
<script>
Vue.component('list-item', {
	props: ['item'],
	template: '<li>{{ item.text }}</li>'
});
var app7 = new Vue({
	el: '#app-7',
	data: {
		list: [
			{text:'a'},
			{text:'b'},
			{text:'c'}
		]
	}
});
</script>
